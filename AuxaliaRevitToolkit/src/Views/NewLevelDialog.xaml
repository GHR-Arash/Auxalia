<Window x:Class="AuxaliaRevitToolkit.Views.NewLevelDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="New Level" Height="200" Width="300">
    <StackPanel Margin="10">
        <Label Content="Name:"/>
        <TextBox x:Name="NameTextBox" Margin="0,5"/>

        <Label Content="Elevation:"/>
        <TextBox x:Name="ElevationTextBox" Margin="0,5"/>
        <Button Content="Create" HorizontalAlignment="Right" Margin="0,10" Click="CreateButton_Click"/>
    </StackPanel>
</Window>

